generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("PGURL")
}

model idapt_docs {
  id      Int     @id @default(autoincrement())
  uid     String  @unique
  collection String
  key     String
  value   Json

  @@map("llamaindx_doc")
}

model idapt_indexes {
  id      Int     @id @default(autoincrement())
  uid     String  @unique
  collection String
  key     String
  value   Json

  @@map("llamaindx_index")
}